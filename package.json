{
  "dependencies": {
    "@tsconfig/node16": "^16.1.3",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "marked": "^14.1.0",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4"
  },
  "name": "markdown-patch",
  "version": "0.2.2",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "docs": "typedoc",
    "docs-serve": "http-server docs"
  },
  "bin": {
    "mdpatch": "./dist/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Change markdown documents by inserting or changing content relative to headings or other parts of a document's structure.",
  "files": [
    "dist/",
    "README.md"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
